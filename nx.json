{
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"affected": {
		"defaultBase": "master"
	},
	"npmScope": "bp",
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint"],
				"parallel": 1
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/angular",
		"warnings": {
			"typescriptMismatch": false,
			"versionMismatch": false
		}
	},
	"generators": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "none",
			"e2eTestRunner": "none"
		},
		"@nrwl/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "none"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		}
	},
	"defaultProject": "weather-forecast"
}
